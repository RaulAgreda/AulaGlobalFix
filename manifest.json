{
    "name": "AulaGlobalStyleFix",
    "version": "1.0",
    "description": "Fixing the inefficient look of AulaGlobal",
    "manifest_version": 3,
    "incognito": "split",
    "icons": {
      "128": "Logo.png"
      },
    "permissions": [
      "activeTab"
    ], 
    "content_scripts": [
        {
            "css": ["style.css"],
            "matches": ["https://aulaglobal.uc3m.es/*"],
            "run_at": "document_start"
        }
    ]
}